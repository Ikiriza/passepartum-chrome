{
    "name": "Passepartum",
    "version": "1.0.0",
    "manifest_version": 3,
    "options_page": "./srcOptions/options.html",
    "icons": {
      "128": "icons/icons8-schlussel-128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [ "icons/icons8-schlussel-128.png", "icons/icons8-key-blue.svg", "icons/icons8-key-white.svg" ],
            "matches": [ "<all_urls>" ]
        }
    ],
    "content_scripts": [{
            "matches": ["https://www.moodle.tum.de/login/index.php", "https://www.moodle.tum.de/"],
            "js": ["passMoodleWelcome.js"]
        },
        {
            "matches": ["https://login.tum.de/*"],
            "js": ["loginShibboleth.js"]
        },
        {
            "matches": ["https://www.reaxys.com/*"],
            "js": ["./passBio/passReaxys.js"]
        },
        {
            "matches": ["https://id.elsevier.com/*"],
            "run_at": "document_end",
            "js": ["./passBio/passElsevir.js"]
        },
        {
            "matches": ["https://www.science.org/*"],
            "run_at": "document_end",
            "js": ["./passBio/passScience.js"]
        },
        {
            "matches": ["https://www.nature.com/*"],
            "js": ["./passBio/passNature.js"]
        }
    ],
    "action": {
        "default_popup": "./srcPopup/popup.html",
        "default_title": "User Input"
    },
    "permissions": [
        "storage"
    ]
}
